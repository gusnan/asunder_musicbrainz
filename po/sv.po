msgid "General"
msgstr "Allmänt"

#: ../src/interface.c:494
msgid ""
"%A - Artist\n"
"%L - Album\n"
"%N - Track number (2-digit)\n"
"%Y - Year (4-digit or \"0\")\n"
"%T - Song title"
msgstr ""
"%A - Artist\n"
"%L - Album\n"
"%N - Spårnummer (2-siffrigt)\n"
"%Y - Årtal (4-siffrigt eller \"0\")\n"
"%T - Låttitel"

